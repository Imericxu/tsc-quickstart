{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/exports-last": "warn",
    "import/first": "warn",
    "import/newline-after-import": "warn",
    "import/order": [
      "warn",
      {
        "alphabetize": {
          "order": "asc"
        },
        "distinctGroup": true,
        "newlines-between": "always"
      }
    ]
  }
}
